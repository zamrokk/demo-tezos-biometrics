{
  "name": "capacitor-native-biometric",
  "version": "4.2.2",
  "description": "This plugin gives access to the native biometric apis for android and iOS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Jose Martinez",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "^3.4.3"
  },
  "devDependencies": {
    "@capacitor/android": "^3.4.3",
    "@capacitor/ios": "^3.4.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorNativeBiometric.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zamrokk/capacitor-native-biometric"
  },
  "bugs": {
    "url": "https://github.com/zamrokk/capacitor-native-biometric/issues"
  }
}
